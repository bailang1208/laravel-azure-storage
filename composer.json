{
    "name": "feilongcui/laravel-azure-storage",
    "description": "Microsoft Azure Blob Storage integration for Laravel's Storage API",
    "type": "library",
    "keywords": ["storage","laravel","azure"],
    "require": {
        "league/flysystem": "~1.0",
        "microsoft/azure-storage-blob": "~1.1.0",
        "microsoft/azure-storage-file": "~1.1.0",
        "microsoft/azure-storage-queue": "~1.1.0",
        "microsoft/azure-storage-table": "~1.1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^6.4",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "^3.1",
        "psy/psysh": "^0.8.13"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "FeiLong Cui",
            "email": "feilongcui@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FeiLongCui\\LaravelAzureStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {

        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FeiLongCui\\LaravelAzureStorage\\AzureStorageServiceProvider"
            ]
        }
    }
}
